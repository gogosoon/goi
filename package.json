{
  "name": "@ggsn/goi",
  "version": "1.0.0",
  "description": "A CLI Tool to convert Table to Typescript Sequelize Model files",
  "author": "Codewithsooners <codewithsooners@gogosoon.com>",
  "keywords": [
    "codewithsooners",
    "generate",
    "typescript-sequelize",
    "model"
  ],
  "license": "Apache-2.0",
  "main": "./lib/index.js",
  "types": "lib/index.d.ts",
  "bin": {
    "goi": "./lib/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gogosoon/goi/"
  },
  "scripts": {
    "dev": "yarn build",
    "refresh": "rm -rf ./node_modules ./package-lock.json ./yarn.lock && npm install",
    "test": "sudo npm i -g && goi",
    "start": "yarn start",
    "lint": "eslint --ext .ts --ext .tsx .",
    "build": "tsc -p . --explainFiles",
    "audit:npm-check-updates": "npx npm-check-updates --errorLevel 1",
    "prepare": "yarn build"
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "child_process": "^1.0.2",
    "clear": "^0.1.0",
    "commander": "^7.1.0",
    "figlet": "^1.5.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^10.0.0",
    "@types/fs-extra": "^9.0.13",
    "path": "^0.12.7",
    "@types/figlet": "^1.5.4",
    "terminal-kit": "2.3.0",
    "@types/sequelize": "^4.28.10",
    "sequelize": "^6.7.0",
    "sequelize-typescript": "^2.1.1",
    "mysql2": "^2.3.2",
    "env-file-reader": "1.0.5"
  },
  "devDependencies": {
    "@babel/code-frame": "^7.16.7",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "chai": "^3.5.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "lint-staged": "^10.5.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "tslint": "^6.1.2",
    "@types/figlet": "^1.5.4",
    "@types/node": "^14.14.33",
    "depcheck": "^1.4.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "files": [
    "lib/*/*"
  ],
  "nodemonConfig": {
    "ignore": [
      "mobile"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "./node_modules/.bin/eslint --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=12"
  }
}
